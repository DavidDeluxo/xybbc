<?xml version="1.0" encoding="UTF-8"?>
<mappings xmlns="http://dozer.sourceforge.net"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
          xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">

    <configuration>
        <date-format>yyyy-MM-dd HH:mm:ss</date-format>
        <map-null>false</map-null>
    </configuration>

    <mapping>
        <class-a>com.xingyun.bbc.order.model.vo.cart.CartsVo</class-a>
        <class-b>com.xingyun.bbc.mallpc.model.vo.shoppingcart.ShoppingCartGoodsVo</class-b>
        <field>
            <a>fqualityStartDate</a>
            <b date-format="yyyy.MM">fqualityStartDate</b>
        </field>
        <field>
            <a>fqualityEndDate</a>
            <b date-format="yyyy.MM">fqualityEndDate</b>
        </field>
    </mapping>

</mappings>
